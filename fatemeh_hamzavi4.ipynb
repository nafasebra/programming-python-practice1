      "source": [
        "st = input('Enter a string: ')\n",
        "\n",
        "for i in range(0, len(st)):\n",
        "  if(st[i].islower()):\n",
        "    st = st.replace(st[i], st[i].upper())\n",
        "  elif(st[i].isupper()):\n",
        "    st = st.replace(st[i], st[i].lower())\n",
        "  else:\n",
        "    pass\n",
        "\n",
        "print(\"Converted string: \", st)"
      ]
    }
