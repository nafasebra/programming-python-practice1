        "def fact1(m):\n",
        "  if m<1:\n",
        "    return 1\n",
        "  else:\n",
        "     return\n",
        "\n",
        "     n=int(input(\"enter n:\"))\n",
        "     x=int(input(\"enter x:\"))\n",
        "     sum=0\n",
        "     for i in range(1,n+1):\n",
        "      sum=sum+((x**(i+2))/(fact1(i+2)))*(-1**i)\n",
        "      print(sum)"
