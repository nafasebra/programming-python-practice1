      "source": [
        "a = input('Enter a string: ')\n",
        "\n",
        "for i in range(0, len(a)):\n",
        "  if(a[i].islower()):\n",
        "    a = a.replace(a[i], a[i].upper())\n",
        "  elif(a[i].isupper()):\n",
        "    a = a.replace(a[i], a[i].lower())\n",
        "  else:\n",
        "    pass\n",
        "\n",
        "print(\"Converted string: \", a)"
      ]
    }
